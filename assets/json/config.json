                          "border": "bdc:208,208,208; bdr:8; bdw:2",
                          "label": {
                            "text": "EMAIL ID",
                            "styleClass": "ft:para1;tc:textSubtle"
                          }
                        }
                      },
                      {
                        "type": "fw/sized_box",
                        "data": { "height": 10 }
                      },
                      {
                        "type": "digia/form/textfield",
                        "data": {
                          "dataKey": "password",
                          "inputType": "password",
                          "hintText": "Enter Password",
                          "textStyle": "ft:para1;tc:text",
                          "border": "bdc:208,208,208; bdr:8; bdw:2",
                          "label": {
                            "text": "PASSWORD",
                            "styleClass": "ft:para1;tc:textSubtle"
                          }
