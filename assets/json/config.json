      "layout": {
        "body": {
          "allowScroll": false,
          "list": {
            "styleClass": "p:sp-300",
            "children": [
              {
                "wrap": false,
                "child": {
                  "type": "digia/image",
                  "data": {
                    "styleClass": "h:200;p:16",
                    "fit": "fitWidth",
                    "imageSrc": "https://d208quqxsjalxm.cloudfront.net/partner-app-images/Login/logo-new.png"
                  }
                }
              },
              {
                "styleClass": "m:0,10,0,0",
